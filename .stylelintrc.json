{
  "extends": "stylelint-config-standard",
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["function", "if", "each", "include", "mixin"]
      }
    ],
    "unit-no-unknown": [
      true,
      {
        "ignoreUnits": ["upx"]
      }
    ]
  },
  "ignoreFiles": ["dist/*.css"]
}
